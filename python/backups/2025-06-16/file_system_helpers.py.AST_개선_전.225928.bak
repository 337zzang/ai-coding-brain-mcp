        with open(file_path, 'r', encoding='utf-8') as f:
            content = f.read()
    except Exception as e:
        return {'found': False, 'error': f'íŒŒì¼ ì½ê¸° ì‹¤íŒ¨: {e}'}
    file_ext = os.path.splitext(file_path)[1].lower()
    if file_ext == '.py':
        if _external_parse_with_snippets:
            try:
                ast_result = _external_parse_with_snippets(file_path, include_snippets=True)
                if not ast_result.get('parsing_success'):
                    return {'found': False, 'error': f"Python íŒŒì‹± ì‹¤íŒ¨: {ast_result.get('error')}"}
            except Exception as parse_err:
                return {'found': False, 'error': f'Python íŒŒì‹± ì˜¤ë¥˜: {parse_err}'}
        else:
            return {'found': False, 'error': 'Python íŒŒì„œë¥¼ ì°¾ì„ ìˆ˜ ì—†ìŒ'}
    elif _external_parse_with_snippets:
        try:
            ast_result = _external_parse_with_snippets(file_path, include_snippets=True)
        except:
            return {'found': False, 'error': 'JavaScript/TypeScript íŒŒì‹± ì‹¤íŒ¨'}
    else:
        return {'found': False, 'error': 'JavaScript/TypeScript íŒŒì„œ ì—†ìŒ'}
    if not ast_result.get('parsing_success'):
        return {'found': False, 'error': f"AST íŒŒì‹± ì‹¤íŒ¨: {ast_result.get('error')}"}
    all_blocks = []
    functions = []
    classes = []
    methods = []
    for func in ast_result.get('functions', []):
        block = {'name': func.get('name'), 'type': 'function', 'line_start': func.get('line_start'), 'line_end': func.get('line_end'), 'col_start': func.get('col_start', 1), 'col_end': func.get('col_end', 1), 'class': None, 'language': ast_result.get('language')}
        all_blocks.append(block)
        functions.append(block)
    for cls in ast_result.get('classes', []):
        class_block = {'name': cls.get('name'), 'type': 'class', 'line_start': cls.get('line_start'), 'line_end': cls.get('line_end'), 'col_start': cls.get('col_start', 1), 'col_end': cls.get('col_end', 1), 'class': None, 'language': ast_result.get('language')}
        all_blocks.append(class_block)
        classes.append(class_block)
        class_name = cls.get('name')
        for method in cls.get('methods', []):
            method_block = {'name': method.get('name'), 'type': 'method', 'line_start': method.get('line_start'), 'line_end': method.get('line_end'), 'col_start': method.get('col_start', 1), 'col_end': method.get('col_end', 1), 'class': class_name, 'language': ast_result.get('language')}
            all_blocks.append(method_block)
            methods.append(method_block)
    if block_name:
        target_class_name = None
        target_func_name = block_name
        if '.' in block_name:
            parts = block_name.rsplit('.', 1)
            if len(parts) == 2:
                target_class_name, target_func_name = parts
        matching_blocks = []
        for block in all_blocks:
            if block['name'] == target_func_name:
                if target_class_name:
                    if block.get('class') == target_class_name:
                        matching_blocks.append(block)
                elif not block.get('class'):
                    matching_blocks.append(block)
        if matching_blocks:
            return {'found': True, 'block': matching_blocks[0], 'blocks': all_blocks, 'functions': functions, 'classes': classes, 'methods': methods, 'total_blocks': len(all_blocks), 'matching_count': len(matching_blocks)}
        else:
            available_names = []
            for b in all_blocks:
                if b.get('class'):
                    available_names.append(f"{b['class']}.{b['name']}")
                else:
                    available_names.append(b['name'])
            return {'found': False, 'error': f'ë¸”ë¡ "{block_name}"ì„ ì°¾ì„ ìˆ˜ ì—†ìŒ', 'available_blocks': available_names}
    return {'found': len(all_blocks) > 0, 'blocks': all_blocks, 'functions': functions, 'classes': classes, 'methods': methods, 'total_blocks': len(all_blocks)}

def replace_block(file_path: str, block_name: str, new_content: str) -> dict:
    """
    íŒŒì¼ì˜ íŠ¹ì • ì½”ë“œ ë¸”ë¡(í•¨ìˆ˜, í´ë˜ìŠ¤, ë©”ì„œë“œ)ì„ ìƒˆë¡œìš´ ë‚´ìš©ìœ¼ë¡œ êµì²´í•©ë‹ˆë‹¤.

    v3.0 ê°œì„ ì‚¬í•­:
    - textwrap.dedentë¥¼ ì‚¬ìš©í•œ ë“¤ì—¬ì“°ê¸° ì •ê·œí™”
    - ê°•í™”ëœ ì»´íŒŒì¼ ê°€ë“œ (compile í•¨ìˆ˜ ì‚¬ìš©)
    - ë”•ì…”ë„ˆë¦¬ í˜•íƒœì˜ ìƒì„¸í•œ ë°˜í™˜ê°’
    - ë¼ì¸ ë¸íƒ€ ì •ë³´ í¬í•¨

    v6.1 ê°œì„ ì‚¬í•­:
    - validate_method_endë¥¼ ì‚¬ìš©í•œ ë©”ì„œë“œ ë ê²€ì¦
    - AST íŒŒì„œ ì˜ì¡´ì„± ì œê±°ë¡œ ê²½ê³  ë©”ì‹œì§€ í•´ê²°

    Args:
        file_path (str): ìˆ˜ì •í•  íŒŒì¼ì˜ ì „ì²´ ê²½ë¡œ
        block_name (str): êµì²´í•  ë¸”ë¡ì˜ ì´ë¦„
                         - í•¨ìˆ˜: 'function_name'
                         - í´ë˜ìŠ¤: 'ClassName'
                         - ë©”ì„œë“œ: 'ClassName.method_name'
        new_content (str): ìƒˆë¡œìš´ ì½”ë“œ ë‚´ìš©

    Returns:
        dict: ì‘ì—… ê²°ê³¼ ë”•ì…”ë„ˆë¦¬
              - status: "SUCCESS" ë˜ëŠ” "ERROR"
              - message: ê²°ê³¼ ë©”ì‹œì§€
              - backup_path: ë°±ì—… íŒŒì¼ ê²½ë¡œ
              - line_delta: ë¼ì¸ ìˆ˜ ë³€ê²½ëŸ‰ (ì¶”ê°€ëœ ê²½ìš° ì–‘ìˆ˜, ì œê±°ëœ ê²½ìš° ìŒìˆ˜)
              - error: ì˜¤ë¥˜ ë°œìƒ ì‹œ ì˜¤ë¥˜ ì •ë³´
    """
    from textwrap import dedent
    new_content = dedent(new_content).strip()
    result = {'status': 'ERROR', 'message': '', 'backup_path': None, 'line_delta': 0, 'error': None}

    if not os.path.exists(file_path):
        result['message'] = f'íŒŒì¼ì´ ì¡´ì¬í•˜ì§€ ì•ŠìŠµë‹ˆë‹¤: {file_path}'
        return result

    # ë°±ì—… ìƒì„±
    backup_path = backup_file(file_path, f'before_replace_{block_name}')
    if 'ERROR' in backup_path:
        result['message'] = backup_path
        return result
    result['backup_path'] = backup_path

    # ë¸”ë¡ ì°¾ê¸°
    blocks_result = find_blocks(file_path, block_name)
    if not blocks_result.get('found'):
        result['message'] = blocks_result.get('error', f"ë¸”ë¡ '{block_name}'ì„ ì°¾ì„ ìˆ˜ ì—†ìŠµë‹ˆë‹¤")
        return result

    block_info = blocks_result['block']
    
    # ë™ì¼í•œ ì´ë¦„ì˜ ë©”ì„œë“œê°€ ìˆëŠ”ì§€ í™•ì¸ (v6.2 - í´ë˜ìŠ¤ ë©”ì„œë“œ ìš°ì„ )
    for method in blocks_result.get('methods', []):
        if method['name'] == block_name:
            block_info = method
            break
    start_line = block_info['line_start']
    end_line = block_info['line_end']

    # íŒŒì¼ ì½ê¸°
    with open(file_path, 'r', encoding='utf-8') as f:
        lines = f.readlines()

    # ë©”ì„œë“œ/í•¨ìˆ˜ì˜ ì‹¤ì œ ë ë¼ì¸ ê²€ì¦ (v6.1 ë²„ê·¸ ìˆ˜ì •)
    validated_end_line = validate_method_end(lines, start_line, end_line, block_info['type'])
    if validated_end_line != end_line:
        print(f"INFO: ë ë¼ì¸ ì¡°ì •: {end_line} â†’ {validated_end_line}")
        end_line = validated_end_line

    # ë“¤ì—¬ì“°ê¸° ê°ì§€ ë° ì ìš©
    col_offset = block_info.get('col_start', 1) - 1
    
    # í´ë˜ìŠ¤ ë©”ì„œë“œëŠ” í‘œì¤€ 4ì¹¸ ë“¤ì—¬ì“°ê¸° ì‚¬ìš© (v6.2)
    if block_info['type'] == 'method' and block_info.get('class'):
        # ë©”ì„œë“œëŠ” ì§ì ‘ 4ì¹¸ ë“¤ì—¬ì“°ê¸° ì ìš©
        processed_content = _apply_method_indentation(new_content)
    else:
        # ì¼ë°˜ì ì¸ ê²½ìš°ëŠ” ê¸°ì¡´ ë¡œì§ ì‚¬ìš©
        col_offset = block_info.get('col_start', 1) - 1
    processed_content = _apply_indentation_from_col(new_content, col_offset)

    # ìƒˆ ë‚´ìš©ì„ ë¼ì¸ ë¦¬ìŠ¤íŠ¸ë¡œ ë³€í™˜
    if processed_content:
        new_lines = processed_content.splitlines(keepends=True)
        if new_lines and (not new_lines[-1].endswith('\n')):
            new_lines[-1] += '\n'
    else:
        new_lines = []

    # ë¼ì¸ êµì²´
    original_line_count = end_line - start_line + 1
    new_line_count = len(new_lines)
    lines[start_line - 1:end_line] = new_lines
    new_file_content = ''.join(lines)

    # ë¼ì¸ ìˆ˜ ë³€ê²½ëŸ‰ ê³„ì‚°
    line_delta = new_line_count - original_line_count

    # Python íŒŒì¼ì¸ ê²½ìš° ë¬¸ë²• ê²€ì¦
    if file_path.endswith('.py'):
        try:
            compile(new_file_content, file_path, 'exec')
            _record_line_delta(file_path, start_line, 0)
        except SyntaxError as e:
            # ë°±ì—…ì—ì„œ ë³µì›
            shutil.copy2(backup_path, file_path)
            result['error'] = str(e)
            result['message'] = f'ìˆ˜ì •ëœ ì½”ë“œì—ì„œ ë¬¸ë²• ì˜¤ë¥˜ ë°œê²¬: {e}'
            return result

    # íŒŒì¼ ì“°ê¸°
    try:
        _atomic_write(file_path, new_file_content)
        result['status'] = 'SUCCESS'
        result['message'] = f"ë¸”ë¡ '{block_name}' êµì²´ ì™„ë£Œ"
        result['line_delta'] = line_delta
        _record_line_delta(file_path, start_line, line_delta)
        _invalidate_and_refresh_cache(file_path)
    except Exception as e:
        # ë°±ì—…ì—ì„œ ë³µì›
        try:
            shutil.copy2(backup_path, file_path)
        except:
            pass
        result['error'] = str(e)
        result['message'] = f'íŒŒì¼ ì“°ê¸° ì‹¤íŒ¨: {e}'

    return result

def insert_block(file_path: str, target_block: str, position: str, new_content: str) -> dict:
    """
    íŒŒì¼ì˜ íŠ¹ì • ë¸”ë¡ ì£¼ë³€ì´ë‚˜ ë‚´ë¶€ì— ìƒˆë¡œìš´ ì½”ë“œë¥¼ ì‚½ì…í•©ë‹ˆë‹¤.

    v4.0 ê°œì„  ë²„ì „:
    - ë°±ì—… ì‹œì  ê°œì„ : ëª¨ë“  ìœ íš¨ì„± ê²€ì‚¬ í›„ ìƒì„±
    - ìœ„ì¹˜ ê³„ì‚° ë¡œì§ ê°œì„ : start/end ëª¨ë“œ ì •í™•ì„± í–¥ìƒ
    - ë“¤ì—¬ì“°ê¸° ë™ì  ì¸¡ì •: ì£¼ë³€ ì½”ë“œ ê¸°ë°˜ ìë™ ì¡°ì •
    """
    from textwrap import dedent
    result = {'status': 'ERROR', 'message': '', 'backup_path': None, 'line_delta': 0, 'error': None}
    if not os.path.exists(file_path):
        result['message'] = f'íŒŒì¼ì´ ì¡´ì¬í•˜ì§€ ì•ŠìŠµë‹ˆë‹¤: {file_path}'
        return result
    valid_positions = ['before', 'after', 'start', 'end', 'after_init']
    if position not in valid_positions:
        result['message'] = f'ìœ íš¨í•˜ì§€ ì•Šì€ position: {position}. ê°€ëŠ¥í•œ ê°’: {valid_positions}'
        return result
    blocks_result = find_blocks(file_path, target_block)
    if not blocks_result.get('found'):
        error_msg = blocks_result.get('error', f'ë¸”ë¡ "{target_block}"ì„ ì°¾ì„ ìˆ˜ ì—†ìŒ')
        result['message'] = error_msg
        return result
    backup_path = backup_file(file_path, f'before_insert_{target_block}_{position}')
    if 'ERROR' in backup_path:
        result['message'] = backup_path
        return result
    result['backup_path'] = backup_path
    block_info = blocks_result['block']
    start_line = block_info['line_start']
    end_line = block_info['line_end']
    base_indent_col = block_info.get('col_start', 1)
    try:
        with open(file_path, 'r', encoding='utf-8') as f:
            lines = f.readlines()
    except Exception as e:
        result['message'] = f'íŒŒì¼ ì½ê¸° ì‹¤íŒ¨: {e}'
        result['error'] = str(e)
        return result
    new_content = dedent(new_content).strip()

    def detect_indent_style(lines, start_idx, end_idx):
        """ì£¼ë³€ ì½”ë“œì—ì„œ ë“¤ì—¬ì“°ê¸° ìŠ¤íƒ€ì¼ ê°ì§€"""
        indent_chars = []
        for i in range(max(0, start_idx), min(len(lines), end_idx)):
            line = lines[i]
            if line.strip():
                indent = line[:len(line) - len(line.lstrip())]
                if indent:
                    indent_chars.append(indent)
        if not indent_chars:
            return '    '
        if any(('\t' in indent for indent in indent_chars)):
            return '\t'
        else:
            min_spaces = min((len(indent) for indent in indent_chars if indent))
            return ' ' * min_spaces
    insert_idx = 0
    indent = ''
    if position == 'before':
        insert_idx = start_line - 1
        indent = ' ' * (base_indent_col - 1)
    elif position == 'after':
        insert_idx = end_line
        indent = ' ' * (base_indent_col - 1)
    elif position == 'start':
        # ë¸”ë¡ ì‹œì‘ ë¶€ë¶„ (í•¨ìˆ˜/í´ë˜ìŠ¤ ë³¸ë¬¸ ì‹œì‘)
        # ì‹¤ì œ ì½”ë“œê°€ ì‹œì‘ë˜ëŠ” ìœ„ì¹˜ ì°¾ê¸°
        found_code_start = False
        idx = start_line  # for ë£¨í”„ ëŒ€ì‹  while ì‚¬ìš©
        
        while idx < min(end_line, start_line + 20):
            line = lines[idx - 1]
            stripped = line.strip()
            
            # def/class ì„ ì–¸ë¶€ ê±´ë„ˆë›°ê¸°
            if idx == start_line:
                idx += 1
                continue
                
            # docstring ê±´ë„ˆë›°ê¸°
            if stripped.startswith('"""') or stripped.startswith("'''"): 
                # docstring ë ì°¾ê¸°
                quote = '"""' if stripped.startswith('"""') else "'''"
                if stripped.count(quote) == 2:  # í•œ ì¤„ docstring
                    idx += 1
                    continue
                else:
                    # ì—¬ëŸ¬ ì¤„ docstring - ëì„ ì°¾ì„ ë•Œê¹Œì§€ ê±´ë„ˆë›°ê¸°
                    idx += 1
                    while idx <= min(end_line, idx + 50):
                        if quote in lines[idx - 1]:
                            idx += 1
                            break
                        idx += 1
                    continue
            
            # ë°ì½”ë ˆì´í„°ë‚˜ ì£¼ì„ ê±´ë„ˆë›°ê¸°
            if stripped.startswith('@') or stripped.startswith('#') or not stripped:
                idx += 1
                continue
            
            # ì‹¤ì œ ì½”ë“œ ì‹œì‘ ìœ„ì¹˜
            insert_idx = idx - 1
            found_code_start = True
            break
        
        if not found_code_start:
            # ì½”ë“œë¥¼ ì°¾ì§€ ëª»í•œ ê²½ìš° í•¨ìˆ˜/í´ë˜ìŠ¤ ì„ ì–¸ ë‹¤ìŒ ì¤„
            insert_idx = start_line
        
        # ë“¤ì—¬ì“°ê¸° ë™ì  ê°ì§€
        indent_unit = detect_indent_style(lines, start_line, end_line)
        if indent_unit == '\t':
            indent = '\t' * ((base_indent_col - 1) // 4 + 1)
        else:
            indent = ' ' * (base_indent_col - 1) + indent_unit
    elif position == 'end':
        insert_idx = end_line - 1
        for i in range(end_line - 1, start_line, -1):
            line = lines[i - 1].strip()
            if line.startswith(('return', 'pass', 'raise', 'yield')):
                insert_idx = i - 1
                break
        indent_unit = detect_indent_style(lines, start_line, end_line)
        if indent_unit == '\t':
            indent = '\t' * ((base_indent_col - 1) // 4 + 1)
        else:
            indent = ' ' * (base_indent_col - 1) + indent_unit
    elif position == 'after_init':
        found_init = False
        for i in range(start_line, end_line):
            line = lines[i - 1].strip()
            if line.startswith('def __init__'):
                init_indent = len(lines[i - 1]) - len(lines[i - 1].lstrip())
                for j in range(i + 1, end_line):
                    next_line = lines[j - 1]
                    if next_line.strip() and len(next_line) - len(next_line.lstrip()) <= init_indent:
                        insert_idx = j - 1
                        found_init = True
                        break
                if found_init:
                    break
        if not found_init:
            insert_idx = start_line
        indent = ' ' * (base_indent_col - 1 + 4)
    indented_lines = []
    for line in new_content.split('\n'):
        if line.strip():
            indented_lines.append(indent + line)
        else:
            indented_lines.append('')
    new_lines = []
    if insert_idx > 0 and lines[insert_idx - 1].strip():
        new_lines.append('')
    new_lines.extend(indented_lines)
    if insert_idx < len(lines) and lines[insert_idx].strip():
        new_lines.append('')
    try:
        lines[insert_idx:insert_idx] = [line + '\n' for line in new_lines]
        with open(file_path, 'w', encoding='utf-8') as f:
            f.writelines(lines)
        result['status'] = 'SUCCESS'
        result['message'] = f'{target_block}ì˜ {position} ìœ„ì¹˜ì— ì½”ë“œë¥¼ ì„±ê³µì ìœ¼ë¡œ ì‚½ì…í–ˆìŠµë‹ˆë‹¤.'
        result['line_delta'] = len(new_lines)
        result['insert_position'] = insert_idx + 1
    except Exception as e:
        result['message'] = f'íŒŒì¼ ì“°ê¸° ì‹¤íŒ¨: {e}'
        result['error'] = str(e)
        try:
            restore_backup(backup_path, file_path)
            result['message'] += ' (ë°±ì—…ì—ì„œ ë³µì›ë¨)'
        except:
            pass
    return result

